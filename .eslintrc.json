{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended",
    "airbnb"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": [],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "react/button-has-type": 0,
    "react/jsx-props-no-spreading": 0,
    "arrow-body-style": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "comma-dangle": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "no-throw-literal": 0,
    "consistent-return": 0,
    "block-scoped-var": 0,
    "no-param-reassign": 0,
    "default-case": 0,
    "operator-linebreak": 0
  }
}
